    "def gcd_extended(a, b):\n",
    "    if a == 0:\n",
    "        return (b, 0, 1)\n",
    "    else:\n",
    "        div, x, y = gcd_extended(b % a, a)\n",
    "    return (div, y - (b // a) * x, x)\n",
    "    \n",
    "\n",
    "\n",
    "gcd, x, y = gcd_extended(int(input()), int(input()))\n",
    "print(gcd)\n",
    "print(x, y)\n",
    "    "
   ]
  }
