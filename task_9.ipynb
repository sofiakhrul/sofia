   "source": [
    "a = int(input(\"Введите число: \"))\n",
    "b = 0\n",
    "for i in range(2, a // 2+1):\n",
    "    if (a % i == 0):\n",
    "        b = b+1\n",
    "if (b <= 0):\n",
    "    print(\"Это простое число\")\n",
    "else:\n",
    "    print(\"Непростое...\")"
   ]
  }
